{
  "name": "faah",
  "displayName": "Faah",
  "description": "When terminal or editor errors get dramatic, Faah gets louder.",
  "icon": "images/icon.png",
  "version": "0.1.2",
  "publisher": "thk",
  "author": "Toufiq Hasan Kiron",
  "repository": {
    "type": "git",
    "url": "https://github.com/kiron0/faah.git"
  },
  "homepage": "https://faah.kiron.dev",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terminalErrorSound.openSettingsUI",
        "title": "Open Settings UI",
        "category": "Faah"
      },
      {
        "command": "terminalErrorSound.playTestSound",
        "title": "Play Test Sound",
        "category": "Faah"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "tsc --noEmit -p ./",
    "watch": "tsc -w -p ./",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:smoke": "vitest run tests/smoke",
    "vscode:prepublish": "npm run compile",
    "package": "vsce package --no-yarn"
  },
  "dependencies": {
    "play-sound": "^1.1.6"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/play-sound": "^1.1.2",
    "@types/vscode": "^1.109.0",
    "@vscode/vsce": "^3.7.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
